{
  "extends": "@virtual-live-lab/tsconfig/remix",
  "compilerOptions": {
    "types": [
      "@remix-run/cloudflare",
      "vite/client",
      "@remix-run/react/future/single-fetch.d.ts"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/.server/**/*.ts",
    "**/.server/**/*.tsx",
    "**/.client/**/*.ts",
    "**/.client/**/*.tsx"
  ]
}
